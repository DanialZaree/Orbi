{
  "name": "my-project",
  "version": "1.0.0",
  "scripts": {
      "format": "prettier --write .",
    "start-server": "cd server && npm start",
"start-client": "cd client && npm run dev -- --host",
    "dev": "concurrently \"npm run start-server\" \"npm run start-client\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1"
  }
}
